{
    "name": "MSCBAM50_org",
    "n_gpu": 1,
    "arch": {
        "type": "MSCBAM50_org",
        "args": {
            "init_band": 3,
            "window": 48
        }
    },
    "data_loader": {
        "type": "LCZh5DataLoader",
        "args": {
            "data_dir": "E:\\ISPRS\\geographic_split/onlyaux\\standard_only_osmdsmlulc_win48_split0.125_test.h5",
            "batch_size": 32,
            "shuffle": true,
            "num_workers": 0
        }
    },
    "val_data_loader": {
        "type": "LCZh5DataLoader",
        "args": {
            "data_dir": "E:\\ISPRS\\geographic_split/onlyaux\\standard_only_osmdsmlulc_win32_split0.125_test.h5",
            "batch_size": 32,
            "shuffle": true,
            "num_workers": 0
        }
    },
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.0003,
            "weight_decay": 0,
            "amsgrad": true
        }
    },
    "loss": "cel_loss",
    "metrics": [
        "accuracy",
        "f1score",
        "f1score_weighted",
        "class_acc"
    ],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 50,
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 100,
        "save_dir": "saved/",
        "save_period": 2,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 10,
        "tensorboard": false
    }
}